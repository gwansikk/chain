{
  "name": "@gwansikk/server-chain",
  "version": "0.5.7",
  "description": "The server-chain is a simple and lightweight HTTP Request library.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "watch": "rollup -cw --bundleConfigAsCjs",
    "build": "pnpm clean && rollup -c --bundleConfigAsCjs",
    "test": "vitest",
    "test:run": "vitest run",
    "clean": "rimraf dist"
  },
  "homepage": "https://github.com/gwansikk/server-chain",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gwansikk/server-chain.git"
  },
  "author": "gwansikk",
  "license": "MIT",
  "workspaces": [
    "docs"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/eslint": "^8",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.4",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "keywords": [
    "fetch",
    "http",
    "request",
    "axios"
  ]
}
